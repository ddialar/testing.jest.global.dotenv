{
    "name": "dotenv-jest-typescript-webpack",
    "version": "1.0.0",
    "description": "Testing code to show how to load a custom Dotenv configuration before the Jest testing suite run.",
    "author": {
        "name": "Dailos Rafael DÃ­az Lara",
        "email": "ddialar@gmail.com"
    },
    "main": "src/service.ts",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/ddialar/testing.jest.global.dotenv",
        "type": "git"
    },
    "engines": {
        "node": ">=12.18.2"
    },
    "scripts": {
        "build:dev": "webpack --config ./webpack/webpack.dev.js",
        "test": "npm run test:clearCache; jest --runInBand --watch",
        "test:noWatch": "npm run test:clearCache; jest --runInBand",
        "test:clearCache": "jest --clearCache",
        "manifest": "node_modules/make-manifest/bin/make-manifest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "pre-push": "npm run test:noWatch"
        }
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
